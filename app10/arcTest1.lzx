<canvas height="200">
    <drawview height="200" width="600">
        <method event="oninit">
            this.arc(100, 100, 75, 0, 90, true);
            this.arc(200, 100, 75, 0, 90, false);
            this.arc(400, 100, 75, 0, 360, false);
            this.stroke();
        </method>
    </drawview>
</canvas>
